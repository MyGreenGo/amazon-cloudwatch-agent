{
    "agent": {
        "metrics_collection_interval": 60,
        "run_as_user": "root"
    },
    "logs": {
        "logs_collected": {
            "files": {
                "collect_list": [
                    {
                        "file_path": "/var/log/greengo/**.log",
                        "log_group_name": "collector-v3",
                        "log_stream_name": "{hostname}",
                        "retention_in_days": 7
                    }
                ]
            }
        }
    }
}